/dts-v1/;

#include "../qcom/sdm845-v2.dtsi"
#include "../qcom/sdm845-mtp.dtsi"
#include "sdm845-sec-common.dtsi"
#include "sdm845-sec-starqlte-pm-common.dtsi"
#include "sdm845-sec-starqlte-pm-r05.dtsi"
#include "sdm845-sec-starqlte-pm-jpn-r05.dtsi"
#include "sdm845-sec-starqlte-r06.dtsi"
#include "sdm845-sec-starqlte-pinctrl-r06.dtsi"
#include "battery-data-starq-md05-common.dtsi"
#include "sdm845-sec-starqlte-display-r01.dtsi"
#include "sdm845-sec-starqlte-displayport-r00.dtsi"
#include "sdm845-sec-starqlte-fingerprint-sensor-r05.dtsi"
#include "sdm845-sec-starqlte-mst-r00.dtsi"
#include "sdm845-camera-sensor-starqlte-r06.dtsi"
#include "sdm845-starq-max77705.dtsi"
#include "sdm845-sec-starqlte-jpn-felica-r00.dtsi"
#include "sdm845-sec-starqlte-jpn-isdbt-r00.dtsi"

/ {
	model = "Samsung STARQLTE JPN PROJECT Rev06";
	compatible = "qcom,sdm845-mtp", "qcom,sdm845", "qcom,mtp";
	qcom,board-id = <8 6>;
};

&snd_934x {
	qcom,msm-mbhc-hs-mic-max-threshold-mv = <1700>;
	qcom,msm-mbhc-hs-mic-min-threshold-mv = <641>;

	detect-extn-cable;
};

&slim_aud {
	imp_list: imp_list {
		#list-imp-cells = <3>;
	};

	tavil_codec {
		imp-table = <&imp_list 0 0 6>,
				 <&imp_list 1 13 1>,
				 <&imp_list 14 25 3>,
				 <&imp_list 26 42 4>,
				 <&imp_list 43 100 7>,
				 <&imp_list 101 200 9>,
				 <&imp_list 201 1000 10>,
				 <&imp_list 1001 0x7fffffff 8>;
	};
};
